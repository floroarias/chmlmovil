{
  "expo": {
    "name": "CHML Mobile",
    "icon": "./assets/chml_azul.png",
    "version": "3.0.0",
    "slug": "CHMLMobile",
    "android": {
      "package": "com.munichos.chmlmobile",
      "versionCode": 14
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "CHML Mobile desea acceder a tus fotos."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.munichos.chmlmobile"
    },
    "privacy": "public",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash_chml.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "android"
    ]
  }
}
