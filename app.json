{
  "expo": {
    "name": "CHML Mobile",
    "icon": "./assets/chml_azul.png",
    "version": "3.0.1",
    "slug": "CHMLMobile",
    "android": {
      "package": "com.munichos.chmlmobile",
      "versionCode": 16,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "CHML Mobile desea acceder a tus fotos."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.munichos.chmlmobile"
    },
    "privacy": "public",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash_chml.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "android"
    ],
    "extra": {
      "eas": {
        "projectId": "082e8ec0-4860-11e9-8c8e-e7a92c53d269"
      }
    }
  }
}
